{
  "name": "Stash Code Coverage",
  "description": "Displays code coverage button on pull requests in Bitbucket Server",
  "version": "1.01",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "lib/kotlin.js",
        "inject.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "options_page": "settings.html",
  "permissions": [
    "storage",
    "*://*/*"
  ],
  "icons": {
    "256": "icons/icon256.png",
    "128": "icons/icon128.png",
    "64": "icons/icon64.png",
    "48": "icons/icon48.png"
  },
  "manifest_version": 2
}